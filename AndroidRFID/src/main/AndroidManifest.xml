<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.arcsoft.sdk_demo"
          xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- camera -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- sdcard -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <!-- wifi internet -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>





    <application
        android:name="com.arcsoft.sdk_demo.activity.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name="com.arcsoft.sdk_demo.activity.PermissionAcitivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.arcsoft.sdk_demo.activity.RegisterActivity"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name="com.arcsoft.sdk_demo.activity.DetecterActivity"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".activity.homeActivity"
            android:screenOrientation="landscape">
        </activity>
        <activity android:name=".activity.setActivity"
            android:screenOrientation="landscape">
        </activity>
    </application>

</manifest>